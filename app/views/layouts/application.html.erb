<script type="text/javascript">
  $(document).on('load', function() {
      $('.expand').on('click', function() {
        $(this).addClass('active');
        console.log("asdasd");
    });
  });
</script>
  </head>
  <body>
    <header>
      <%= render 'layouts/navigation' %>
